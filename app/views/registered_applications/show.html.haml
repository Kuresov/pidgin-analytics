.col-md-3

.col-md-6
  %h1.text-center= @application.name
  %h4.text-center= @application.url
  %table.table
    %thead
      %tr
        %th Event Name
        %th Count
    %tbody
      - @events.each do |key,val|
        %tr
          %td= key
          %td= val.count
  %h4.text-center Your client side tracking code:
  %p
    %pre
      :preserve
        var pidgin = {
          report: function(eventName){
            var event = { name: eventName }
            var request = new XMLHttpRequest();

            request.open("POST", "http://localhost:3000/api", true);
            request.setRequestHeader('Content-Type', 'application/json');
            request.send(JSON.stringify(event));

            request.onreadystatechange = function() {
              console.log("In the mix")
            };
          }
        };

        pidgin.report("Visit");
.col-md-3
